<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CoreInzideTheme">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="CoreinzidePro">
        <item name="android:background">@drawable/coreinzide_on</item>
    </style>
    <style name="bigTitle" parent="android:TextAppearance">
        <item name="android:textSize">20dip</item>
        <item name="android:textColor">@color/blue</item>
    </style>
    <style name="midTitle" parent="android:TextAppearance">
        <item name="android:textSize">20dip</item>
        <item name="android:textColor">@color/green</item>
    </style>
    <style name="secTitle" parent="android:TextAppearance">
        <item name="android:textSize">18dip</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
</resources>
